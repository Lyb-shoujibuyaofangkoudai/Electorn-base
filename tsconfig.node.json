{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/**/*",
    "src/**/*"
  ],
  "compilerOptions": {
    "noUnusedLocals": false,  // 禁用未使用局部变量的检查
    "noUnusedParameters": false,  // 禁用未使用参数的检查
    "composite": true,
    "types": [
      "electron-vite/node"
    ]
  }
}
