    "axios": "^1.7.9",
    "colord": "^2.9.3",
    "dayjs": "^1.11.13",
    "electron-updater": "^6.1.7",
    "js-yaml": "^4.1.0",
    "p-queue": "^8.1.0",
    "pinia": "^2.3.0",
    "reflect-metadata": "^0.2.2",
    "regedit": "^5.1.3",
    "sqlite3": "5.1.6",
    "typeorm": "^0.3.20",
    "vue-router": "4",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
