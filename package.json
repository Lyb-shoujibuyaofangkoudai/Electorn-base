    "electron": "^31.0.2",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "naive-ui": "^2.41.0",
    "prettier": "^3.3.2",
    "sass": "^1.83.1",
    "terser": "^5.38.1",
    "typescript": "^5.5.2",
    "unocss": "^0.65.3",
    "unocss-preset-theme": "^0.14.1",
    "unplugin-auto-import": "^19.0.0",
    "unplugin-vue-components": "^28.0.0",
